%CONFIG     {COMMAND CHAR} {%}
%CONFIG     {CONVERT META} {OFF}
%CONFIG     {HIGHLIGHT} {ON}

/* IPv4 */
%HIGHLIGHT  {((25[0-5]|(2[0-4]|[0-1]?[0-9])?[0-9])\.){3}(25[0-5]|(2[0-4]|[0-1]?[0-9])?[0-9])(\/(3[0-2]|[0-2]?[0-9]))?} {cyan} {5}

/* IPv6 */
%HIGHLIGHT  {(([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:[0-9a-fA-F]{0,4}){0,4}%[0-9a-zA-Z]+|::(ffff(:0{1,4})?:)?((25[0-5]|(2[0-4]|[0-1]?[0-9])?[0-9])\.){3}(25[0-5]|(2[0-4]|[0-1]?[0-9])?[0-9])|([0-9a-fA-F]{1,4}:){1,4}:((25[0-5]|(2[0-4]|[0-1]?[0-9])?[0-9])\.){3}(25[0-5]|(2[0-4]|[0-1]?[0-9])?[0-9]))(\/(12[0-8]|1[0-1][0-9]|[0-9]{1,2}))?} {cyan} {5}

/* MAC */
%HIGHLIGHT  {([0-9a-fA-F]{4}\.){2}[0-9a-fA-F]{4}|([0-9a-fA-F]{2}:){5}[0-9a-fA-F]{2}} {yellow} {5}

/* Password or password */
%HIGHLIGHT  {(P|p)assword} {red} {5}
