/* IPv4 */
HIGHLIGHT {(?i)(?<=[\W]|^)(?<!\.)(?:(?P<ip>25[0-5]|(?:2[0-4]|[0-1]?\d)?\d)\.){3}(?P>ip)(?:/\d*)?(?!\.)(?=[\W]|$)} {cyan} {1000}

/* IPv6 */
HIGHLIGHT {(?i)(?<=[\W]|^)(?<!:)(?:::(?:ffff(?::0{1,4})?:)?(?:(?P<ip>25[0-5]|(?:2[0-4]|[0-1]?\d)?\d)\.){3}(?P>ip)|(?:(?P<h>[\da-f]{1,4}):){1,4}:(?:(?P>ip)\.){3}(?P>ip)|(?:(?P>h):){7}(?P>h)|(?P>h):(?:(?::(?P>h)){1,6})|(?:(?P>h):){1,2}(?::(?P>h)){1,5}|(?:(?P>h):){1,3}(?::(?P>h)){1,4}|(?:(?P>h):){1,4}(?::(?P>h)){1,3}|(?:(?P>h):){1,5}(?::(?P>h)){1,2}|(?:(?P>h):){1,6}:(?P>h)|:(?:(?::(?P>h)){1,7}|:)|(?:(?P>h):){1,7}:)(?:%[\da-z]+)?(?:/\d*)?(?!:)(?=[\W]|$)} {magenta} {900}

/* MAC */
HIGHLIGHT {(?i)(?<=[\W]|^)(?<!:)(?:(?:(?P<h>[\da-f])(?P>h)?:){5}(?P>h){1,2}|(?:(?P>h){4}\.){2}(?P>h){4})(?!:)(?=[\W]|$)} {<bbd>} {1000}

/* Password or password */
HIGHLIGHT {(?i)password} {red} {1000}

/* Date in (YYYY-MM-DD|MMM (YYYY|DD)|DD MMM (YYYY)?)[\WT_]hh:mm:ss.SSSSSS format (date, sec, msec, and nsec optional) */
HIGHLIGHT {(?i)(?:(?:\d{4}\-(?:(?:0)?[1-9]|1[0-2])\-(?:3[0-1]|(?:[1-2]\d)|(?:0)?[1-9])|(?P<MMM>jan|feb|mar|apr|may|jun|jul|aug|sep|oct|nov|dec)\s(?:\d{4}|\s\d|(?:3[0-1]|(?:[0-2]\d)))|(?:\d|(?:3[0-1]|(?:[0-2]\d)))\s(?P>MMM)(?:\s\d{4})?)[\WT_])?(?:(?:2[0-3])|[0-1]\d):[0-5]\d(?::[0-5]\d)?(?:\.\d{3,6})?} {<DBA>} {1000}

/* Juniper interfaces */
HIGHLIGHT {(?i)(?<=[\W]|^)(?:(?:(?:fe|ge|xe|et|gr|ip|lt|lsq|mt|sp|vcp)\-\d*/\d*/\d*)|(?:(?:(?:b)?me|em|fab|fxp|fti|lo|pp(?:d|e)?|st|swfab)[0-2]|dsc|gre|ipip|irb|jsrv|lsi|mtun|pimd|pime|tap|vlan|vme|vtep)|(?:(?:ae|reth)\d*))(?:\.\d*)?(?=[\W]|$)} {<fcc>} {1000}

/* Cisco interfaces */
HIGHLIGHT {(?i)(?<=[\W]|^)(?:(?:(?:Hu(?:ndredGigabit)?|Fo(?:rtyGigabit)?|Te(?:nGigabit)?|Gi(?:gabit)?|Fa(?:st)?)(?:Ethernet)?)|Eth|Se(?:rial)?|Lo(?:opback)?|Tu(?:nnel)?|VL(?:AN)?|Po(?:rt-channel)?|Vi(?:rtual\-(?:Template|Access))?|Mu(?:ltilink)?|Di(?:aler)?|(?:B|N)VI)(?:(?:\d*/){0,2}\d*)(?:\.\d*)?(?=[\W]|$)} {<fcc>} {1000}

/* Generics */
HIGHLIGHT {(?i)(?<=[\W]|^)(?:up|ok(?:ay)?|permit(?:ed|s)?|accept(?:s|ed)?|enable(?:d)?|online|succe(?:(?:ss(?:ful|fully)?)|ed(?:ed)?)?|connect(?:ed)?|reachable|valid|forwarding|synchronized)(?=[\W]|$)} {green} {1000}
HIGHLIGHT {(?i)(?<=[\W]|^)(?:ye(?:s|a(?:h)?|p)?|started|running|can)(?=[\W]|$)} {<aba>} {1000}
HIGHLIGHT {(?i)(?<=[\W]|^)(?:warning(?:s)?)(?=[\W]|$)} {yellow} {1000}
HIGHLIGHT {(?i)(?<=[\W]|^)(?:no(?:t|pe)?|exit(ed)?|reset|filter(?:ed)?|stop(?:p(?:ed|ing))?|never|can(?:(?:')?t|not))(?=[\W]|$)} {<cba>} {900}
HIGHLIGHT {(?i)(?<=[\W]|^)(?:abnormal(?:ly)?|down|los(?:t|s|ing)|err(?:or(?:s)?)?|(?:den(?:y|ies|ied)?)|reject(?:ing|ed)?|drop(?:ped|s)?|(?:err\-)?disable(?:d)?|(?:time(?:d)?(?:\-)?out)|fail(?:s|ed|iure)?|disconnect(?:ed)?|unreachable|invalid|bad|notconnect|unusable|block(?:ing|ed)?|blk|inaccessible|wrong|collision(?:s)?|unsynchronized|mismatch|runts|CRC)(?=[\W]|$)} {red} {1000}

/* Half-duplex */
HIGHLIGHT {(?i)(?<=[\W]|^)(?:half(?:\-)?duplex)(?=[\W]|$)} {red} {1000}

/* Cisco Syslog facilities */
HIGHLIGHT {(?i)(?<=[\W]|^)(?:%\w*\-[0-3]\-\w*)(?=[\W]|$)} {red} {1000}
HIGHLIGHT {(?i)(?<=[\W]|^)(?:%\w*\-[4-5]\-\w*)(?=[\W]|$)} {yellow} {1000}
HIGHLIGHT {(?i)(?<=[\W]|^)(?:%\w*\-[6-7]\-\w*)(?=[\W]|$)} {<bef>} {1000}

/* Cisco spanning tree */
HIGHLIGHT {(?<=[\W]|^)(?:BKN|(?:LOOP|ROOT|TYPE|PVID)_Inc)(?=[\W]|$)} {red} {1000}
HIGHLIGHT {(?<=[\W]|^)(?:FWD|Root|Desg)(?=[\W]|$)} {green} {1000}

/* OSPF neighbor states (?:2WAY is normal with more than 3 routers on broadcast domain) */
HIGHLIGHT {(?<=[\W]|^)(?:ATTEMPT|INIT|EXCHANGE|LOADING)(?=[\W]|$)} {yellow} {1000}
HIGHLIGHT {(?<=[\W]|^)(?:2WAY|FULL)(?=[\W]|$)} {green} {1000}

/* BGP neighbor states */
HIGHLIGHT {(?<=[\W]|^)(?:Idle|Connect|Active|OpenSent|OpenConfirm)(?=[\W]|$)} {yellow} {1000}
